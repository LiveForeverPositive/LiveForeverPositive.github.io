range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n    <!---->\n    <!--Overlay-->\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type!== 'color'\" mbr-style=\"{'opacity': overlayOpacity, 'background-color': overlayColor}\">\n    </div>\n    <!--Container-->\n    <div class=\"container\">\n        <div class=\"col-md-12\">\n            <div class=\"media-container-row\">\n                <div class=\"mbr-figure\" mbr-style=\"{'width': mediaSize + '%'}\">\n                    <img src=\"@PROJECT_PATH@/assets/images/untitled-design-6-1046x1395.png\" alt=\"Mobirise\" title>\n                </div>\n                <div class=\"align-left aside-content\">\n                    <h2 class=\"mbr-title pt-2 mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\">Lip Balm</h2>\n                    <div class=\"mbr-section-text\">\n                        <p class=\"mbr-text text1 pt-2 mbr-light mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".text1\">This Lip Balm/Chapstick is a homemade recipe. This will relax and moisturize your dry lips. With intense flavor and scent. \n<br>\n<br><b>Two Flavors</b>:\n<br>Cherry \n<br>Peppermint\n<br>\n<br><b>This Product Is:</b> \n<br>Gluten Free \n<br>BPA Free \n<br>Made in the U.S.A <br>------------------------------ \n<br>This item comes with a Free Sticker!</p>\n                        <p class=\"mbr-text text2 pt-4 mbr-light mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"showSubtitle2\" data-app-selector=\".text2, .mbr-section-btn\">\n                            $3.50 USD<br></p>\n                    </div>\n                   <center>\n                                 <div id=\"lipbalms\"></div>\n<script src=\"https://www.paypal.com/sdk/js?client-id=AfeOZ4xxxG7ZcrtNnKedh6Wm6cRsPdCEV6QtuYItaNd1FXyXdq-wpHxvB-J-xZOkSjp3WF7SG0Qum8zZ&currency=USD\" data-sdk-integration-source=\"button-factory\"></script>\n<script>\n  paypal.Buttons({\n      style: {\n          shape: 'rect',\n          color: 'blue',\n          layout: 'vertical',\n          label: 'paypal',\n          \n      },\n      createOrder: function(data, actions) {\n          return actions.order.create({\n              purchase_units: [{\n                  amount: {\n                      value: '3.50'\n                  }\n              }]\n          });\n      },\n      onApprove: function(data, actions) {\n          return actions.order.capture().then(function(details) {\n              alert('Transaction completed by ' + details.payer.name.given_name + '!');\n          });\n      }\n  }).render('#lipbalms');\n</script>\n\n\n\n                                  </center>\n\n                    <!--Btn-->\n                    <div mbr-buttons mbr-theme-style=\"display-4\" class=\"mbr-section-btn pt-3\" mbr-if=\"showButtons\" data-toolbar=\"-mbrBtnMove\" mbr-class=\"{'align-left' : alignBtn == 1, 'align-center' : alignBtn == 2, 'align-right' : alignBtn == 3}\">\n                        <a href=\"https://mobirise.co\" class=\"btn btn-warning-outline\">\n                            Buy Now\n                        </a>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
